n'\n\nexport default props => {\n    \n    const renderRows = () => {\n        const list = props.list || []\n        return (\n            list.map(todo => (\n                <tr key={todo._id}>\n                    <td className={todo.done ? 'markedAsDone' : ''} >{todo.description}</td>\n                    <td>\n                        \n                        <IconButton style=\"success\" icon=\"check\" hide={todo.done} onClick={() => props.handleMarkAsDone(todo)}></IconButton>\n                        <IconButton style=\"warning\" icon=\"undo\" hide={!todo.done} onClick={() => props.handleMarkAsPending(todo)}></IconButton>\n                        <IconButton style=\"danger\" icon=\"trash-o\" hide={!todo.done} onClick={() => props.handleRemove(todo)}></IconButton>\n                    </td>\n                </tr>\n            ))\n        )\n}\n\n    return (\n        <table className=\"table\">\n            <thead>\n                <tr>\n                    <th>Descrição</th>\n                    <th className=\"tableActions\">Ações</th>\n                </tr>\n            </thead>\n            <tbody>\n                {renderRows()}\n            </tbody>\n        </table>\n    )\n}","/home/erick/Documentos/React/TodoApp/frontend/src/todo/todoForm.jsx",["70","71","72","73"],"\nimport React from 'react'\nimport Grid from '../template/grid'\nimport IconButton from '../template/iconButton'\n\nexport default props => {\n    const keyHandler = (e) => {\n        if (e.key === 'Enter') {\n            e.shiftKey ? props.handleSearch() : props.handleAdd()\n        } else if (e.key === 'Escape') {\n            props.handleClear()\n        }\n    }\n\n    return (\n        <div role=\"form\" className=\"todoForm mt-3 mb-3\" style={{ display: 'flex' }}>\n            \n            <div className=\"formInput\">\n            <Grid cols=\"12 9 10\">\n                <input id=\"description\"\n                    className=\"form-control\"\n                    placeholder=\"Adicione uma tarefa\"\n                    onChange={props.handleChange}\n                    onKeyUp={keyHandler}\n                    value={props.description}>\n                </input>\n            </Grid>\n            </div>\n\n\n            <Grid className=\"buttons\" cols='12 3 2'>\n                <IconButton style=\"primary\" icon=\"plus\" onClick={props.handleAdd}></IconButton>\n                <IconButton style=\"info\" icon='search' onClick={props.handleSearch}></IconButton>\n                <IconButton style=\"warning\" icon=\"close\" onClick={props.handleClear}></IconButton>\n            </Grid>\n        </div>\n    )\n}","/home/erick/Documentos/React/TodoApp/frontend/src/template/grid.jsx",[],"/home/erick/Documentos/React/TodoApp/frontend/src/template/iconButton.jsx",["74","75"],"import react from 'react'\nimport If from './if'\n\nexport default props => (\n    <If test={!props.hide}>\n            <button className={'btn btn-' + props.style}\n            onClick={props.onClick}>\n                <i className={'fa fa-' + props.icon}></i>\n            </button>\n    </If>\n)","/home/erick/Documentos/React/TodoApp/frontend/src/template/if.jsx",["76","77"],"import React from 'react'\n\nexport default props => {\n    if(props.test) {\n        return props.children\n    } else {\n        return false\n    }\n}",["78","79"],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":4,"column":1,"nodeType":"86","endLine":14,"endColumn":2},{"ruleId":"87","severity":1,"message":"88","line":8,"column":17,"nodeType":"89","endLine":8,"endColumn":54},{"ruleId":"84","severity":1,"message":"85","line":7,"column":1,"nodeType":"86","endLine":13,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":3,"column":1,"nodeType":"86","endLine":7,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":4,"column":1,"nodeType":"86","endLine":36,"endColumn":2},{"ruleId":"90","severity":1,"message":"91","l